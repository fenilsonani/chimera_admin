<%- contentFor('HeaderCss') %>

<%- contentFor('body') %>

<h3>Question Sets</h3>

<div class="row">
    <% questionSets.forEach(function(questionSet) { %>
        <div class="col-xl-4 col-md-6">
            <!-- card -->
            <div class="card card-animate">
                <div class="card-body">
                    <div class="d-flex align-items-center">
                        <div class="flex-grow-1 overflow-hidden">
                            <h5 class="text-uppercase fw-medium text-muted text-truncate mb-0"><%= questionSet.name %></h5>
                            <p class="text-bold mb-0"><%= questionSet.totalQuestions %> Questions</p>
                        </div>
                        <!--                                        <div class="flex-shrink-0">-->
                        <!--                                            <h5 class="text-success fs-14 mb-0">-->
                        <!--                                                <%= questionSet.createdAt %>-->
                        <!--                                            </h5>-->
                        <!--                                        </div>-->
                    </div>
                    <div class="d-flex align-items-end justify-content-between mt-4">
                        <h5 class="fs-14 mb-0">
                            <span class="text-Bold "> Created On :</span> <span> <%= questionSet.createdAt %></span>
                        </h5>
                        <a href="questionSet/<%= questionSet._id %>" class="btn btn-outline-primary btn-sm">View Details</a>
                        <a href="DeletequestionSet/<%= questionSet._id %>" class="btn btn-outline-danger btn-sm">Delete</a>
                    </div>
                </div><!-- end card body -->
            </div><!-- end card -->
        </div>
    <% }); %>
        <% if (questionSets.length === 0) { %>
            <div class="col-xl-4 col-md-6">
                <!-- card -->
                <div class="card card-animate">
                    <div class="card-body">
                        <div class="d-flex align-items-center">
                            <div class="flex-grow-1 overflow-hidden">
                                <h5 class="text-uppercase fw-medium text-muted text-truncate mb-0">No Question Sets</h5>
                            </div>
                        </div>
                    </div><!-- end card body -->
                </div><!-- end card -->
            </div>
        <% } %>
</div>


